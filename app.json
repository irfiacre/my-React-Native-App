{
  "expo": {
    "name": "MyApp",
    "slug": "music-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/music-icon.png",
    "entryPoint": "./index.js",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/music-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/music-icon.png"
    },
    "description": "This is my first App, where I am learning React-Native",
    "githubUrl": "https://github.com/irfiacre/my-React-Native-App"
  }
}
